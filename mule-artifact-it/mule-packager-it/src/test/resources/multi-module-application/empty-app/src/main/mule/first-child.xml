<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:spring="http://www.springframework.org/schema/beans" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">
    <flow name="first-childFlow">
        <logger level="INFO" doc:name="Logger"/>
        <flow-ref name="first-childSub_Flow" doc:name="first-childSub_Flow"/>
    </flow>
    <sub-flow name="first-childSub_Flow">
        <flow-ref name="first-childSub_Flow1" doc:name="first-childSub_Flow"/>
    </sub-flow>
    <sub-flow name="first-childSub_Flow1">
        <logger level="INFO" doc:name="Logger"/>
    </sub-flow>
</mule>
